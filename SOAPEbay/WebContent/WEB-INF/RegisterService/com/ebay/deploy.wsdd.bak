<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from RegisterService WSDL service -->

  <service name="Register" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://ebay.com"/>
      <parameter name="wsdlServiceElement" value="RegisterService"/>
      <parameter name="schemaQualified" value="http://ebay.com"/>
      <parameter name="wsdlServicePort" value="Register"/>
      <parameter name="className" value="com.ebay.RegisterSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Register"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="registerUser" qname="operNS:registerUser" xmlns:operNS="http://ebay.com" soapAction="" >
        <parameter qname="pns:email" xmlns:pns="http://ebay.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:firstName" xmlns:pns="http://ebay.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:lastName" xmlns:pns="http://ebay.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:mobile" xmlns:pns="http://ebay.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:password" xmlns:pns="http://ebay.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="registerUser"/>

  </service>
</deployment>
