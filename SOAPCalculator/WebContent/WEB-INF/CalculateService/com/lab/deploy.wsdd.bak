<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculateService WSDL service -->

  <service name="Calculate" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://lab.com"/>
      <parameter name="wsdlServiceElement" value="CalculateService"/>
      <parameter name="schemaQualified" value="http://lab.com"/>
      <parameter name="wsdlServicePort" value="Calculate"/>
      <parameter name="className" value="com.lab.CalculateSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Calculate"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="calculate" qname="operNS:calculate" xmlns:operNS="http://lab.com" returnQName="retNS:calculateReturn" xmlns:retNS="http://lab.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:op1" xmlns:pns="http://lab.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:operator" xmlns:pns="http://lab.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:op2" xmlns:pns="http://lab.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="calculate"/>

  </service>
</deployment>
